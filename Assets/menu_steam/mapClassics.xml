	<script>
		sequence backToMenu()
		{
			playSound('menu_select');
			fadeOut(0.5, true);
			playMusic('music/menu');
			setPage('main_menu');
			unbindMaterial('/Screens/map/map_classics');
			waitFrame(2);
			fadeIn(0.5, true);
		}
		function mouseEnterBack()
		{
			playSound('menu_move');
			setButtonState(1);
			stopGlow();
		}
		sequence backToMapPage()
		{
			playSound('menu_select');
			fadeOut(0.5, false);
			setPage('map');
			unbindMaterial('/Screens/map/map_classics');
			waitFrame(2);
